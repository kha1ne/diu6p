  ],
  "homepage": "https://mmaric.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kha1ne/diu6p.git"
  },
  "bugs": {
    "url": "https://github.com/kha1ne/diu6p/issues"
  },
